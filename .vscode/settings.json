{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "cSpell.words": ["PWSC", "viss"],
  "cSpell.ignoreWords": [
    "Catergory",
    "PaystackButton",
    "PaystackPaymentButton",
    "PaystackProps",
    "Swiper",
    "Tanstack",
    "firstname",
    "lastname",
    "persistor",
    "signup",
    "toastify"
  ],
  "editor.fontSize": 13,
  "editor.fontFamily": "Source Code Pro"
}
